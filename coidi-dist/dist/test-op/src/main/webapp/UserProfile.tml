<html t:title="message:title" t:type="layout"
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd" xmlns:p="tapestry:parameter">
   	<h2>User name: ${user.name}</h2>
   	<br />
   	Identities:
   	<t:loop source="user.identities" value="identity">
   		<div>	
   			<h3>Identity: ${identity.idIdentity}</h3>
   			<p>Nickname: ${identity.nickname}</p>
   			<p>email: ${identity.email}</p>
   			<p>fullname: ${identity.fullname}</p>
   			<p>dob: ${identity.dob}</p>
   			<p>gendre: ${identity.gendre}</p>
   			<p>postcode: ${identity.postcode}</p>
   			<p>country: ${identity.country}</p>
   			<p>language: ${identity.language}</p>
   			<p>timezone: ${identity.timezone}</p>
   		</div>
   	</t:loop>
</html>