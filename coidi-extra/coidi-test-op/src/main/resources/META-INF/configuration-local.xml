<configuration>
	<op>
		<property name="openid.version11.enabled">true</property>
		<property name="server">http://localhost:8080/</property>
		<property name="identity.pattern">http://localhost:8080/identity/%identity%</property>
		<property name="fill.with.data.on.startup">true</property>
		<property name="init.sql.data">src/main/resources/data.sql</property>
		<property name="stateless.mode.association.type">HMAC-SHA1</property>
		<property name="nonce.timeToLiveInSeconds">108000</property>
		<property name="err.contact">john@somesite.com</property>
		<property name="noEncryptionSessionTypeEnabled">false</property>
	</op>
</configuration>
