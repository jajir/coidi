<configuration>
	<op>
		<property name="openid.version11.enabled">true</property>
		<property name="fill.with.data.on.startup">false</property>
		<property name="init.sql.data">none</property>
		<property name="server">http://localhost:8080/</property>
		<property name="idenity.prefix">http://localhost:8080/user</property>
		<property name="idenity.pattern">http://localhost:8080/user/${identity}</property>
		<property name="stateless.mode.association.type">HMAC-SHA1</property>
		<property name="nonce.timeToLiveInSeconds">1800</property>
		<property name="err.contact">john@somesite.com</property>		
		<property name="wild-card.in.realm.enabled">true</property>		
	</op>
</configuration>
